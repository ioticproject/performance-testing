# GENERAL
VERBOSE=0

# BENCHMARKING
# HOST=192.168.100.181
# EXPOSED_PORT=80
HOST=host.docker.internal
EXPOSED_PORT=5000

ADMIN_PASSWORD="!!new_Password"
ADMIN_USERNAME="admin"


HEALTH=api/health

# USER ROUTES
AUTH=auth

USER_LOGIN=api/users/login
GET_USERS=api/users
GET_USER=api/users/{ID}
ADD_USER=api/users
DELETE_USER=api/users/{ID}
UPDATE_USER=api/users/{ID}


# DEVICE_ROUTES
GET_DEVICES=api/devices
ADD_DEVICE=api/users/{USER_ID}/devices
