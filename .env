DELETE_SENSORS=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors/{ID}
UPDATE_SENSORS=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors/{ID}
GET_DEVICE_SENSORS=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors
GET_USER_SENSORS=api/users/{USER_ID}/sensors


# DATA_ROUTES
GET_DATA=api/data
ADD_DATA=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors/{SENSOR_ID}/data
DELETE_DATA=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors/{SENSOR_ID}/data/{ID}
GET_SENSOR_DATA=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors/{SENSOR_ID}/data
GET_FILTERED_SENSOR_DATA=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors/{SENSOR_ID}/data/filter


