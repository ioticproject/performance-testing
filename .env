GET_DEVICE=api/users/{USER_ID}/devices/{ID}
UPDATE_DEVICE=api/users/{USER_ID}/devices/{ID}
GET_USER_DEVICES=api/users/{USER_ID}/devices


# SENSOR_ROUTES
GET_SENSORS=api/sensors
GET_SENSOR=api/users/{USER_ID}/devices/{DEVICE_ID}/sensors/{ID}
